%%% Motivation %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\headerbox{Background}{name=background,column=0,span=1,row=0}{
\small
    \begin{itemize}[leftmargin=1.4em]
        \item \textbf{NR-U}
        \vspace{-4pt}
        \begin{itemize}[leftmargin=1em]
        \vspace{-2pt}\item[$\blacktriangleright$]  Support $2.4$ GHz, $5$ GHz and $6$ GHz unlicensed spectra.
        \vspace{-2pt}\item[$\blacktriangleright$]  Higher spectrum efficiency and more reliable connections than the basic WiFi networks.
        \end{itemize}

        \vspace{-5pt}
        \item \textbf{WiFi}
        \vspace{-4pt}
        \begin{itemize}[leftmargin=1em]
        \vspace{-2pt}\item[$\blacktriangleright$]  Widely applications, economy, and ease to deployment.
        \vspace{-2pt}\item[$\blacktriangleright$]  OFDMA and the uplink MU-MIMO makes \textbf{IEEE 802.11ax standard} \textbf{closer to the cellular protocol}.
        \end{itemize}

        \vspace{-5pt}
        \item \textbf{Coexistence under network slicing}
        \vspace{-4pt}
        \begin{itemize}[leftmargin=1em]
          \vspace{-2pt}  \item[$\blacktriangleright$] A multiple logical network architecture can offer efficient network connectivity for heterogeneous RATs.
          \vspace{-2pt}  \item[$\blacktriangleright$] WiFi network can be merged into the multiple logical network \textbf{as a network slice}.
          \vspace{-2pt}  \item[$\blacktriangleright$] How to ensure \textbf{harmonious coexistence} between WiFi and other RATs?
        \end{itemize}
    \end{itemize}

}

%%% Overview %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\headerbox{Contribution}{name=contribution,column=1,span=1, row = 0,bottomaligned=background}{
\small {A \emph{synchronous RAT} is proposed based on \textbf{network slicing} and \textbf{spatial multiplexing}.} \\
\vspace{-6pt}
\begin{itemize}[leftmargin=1.4em]
    \vspace{-4pt}
     \item \textbf{A new framework}
     \begin{itemize}[leftmargin=1.2em]
        \vspace{-6pt}\item[$\blacktriangleright$] WiFi and NR-U networks are integrated into one logical network through network slicing.
        \vspace{-4pt}\item[$\blacktriangleright$] Provide efficient network connectivity while protecting WiFi performance.
     \end{itemize}
    \vspace{-4pt}
    \item \textbf{A synchronous RAT}
    \begin{itemize}[leftmargin=1.2em]
        \vspace{-6pt}\item[$\blacktriangleright$] Support synchronous NR-U and WiFi transmissions with spatial diversity.
        \vspace{-4pt}\item[$\blacktriangleright$] Spectrum efficiency is improved through NR-U and WiFi coordination.
    \end{itemize}
    \vspace{-4pt}
    \item \textbf{A suboptimal \textbf{semiorthogonal user group construction scheme}}
    \begin{itemize}[leftmargin=1.2em]
        \vspace{-6pt}\item[$\blacktriangleright$] Maximize unlicensed resource utility while maintaining certain level of WiFi \& cellular users' performance based on users' specific        requirements.
        \vspace{-15pt}\item[$\blacktriangleright$] Low complexity and asymptotically optimal performance.
    \end{itemize}
\end{itemize}
}

%%% Method %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\headerbox{Slicing based framework}{name=model,column=2, span=1, row = 0, bottomaligned=contribution}{
\vspace{-2pt}
\begin{minipage}[c]{\textwidth}
    \centering
    \includegraphics[width=0.78\textwidth]{fig/sysmodel}
\end{minipage}\\
 \vspace{-8pt}
\begin{itemize}[leftmargin=1 em]
\small{
\item WiFi integrated into NR-U as a slice. \vspace{-6pt}
\item Cellular network is divided into several slices.\vspace{-6pt}
\item Cellular/WiFi users associate gNB based on 3GPP protocol/IEEE-compliant standards.
}
\end{itemize}
}

\headerbox{Signal model}{name=signal,column=0, span=1, below=background}{
\begin{minipage}[c]{\textwidth}
\small
\begin{itemize}[leftmargin=1.4 em]
 \vspace{-2pt}
\item The downlink transmit signal vector is given by
 \vspace{-2pt}
\begin{equation}\notag
\mathbf{x} = \sum_{k \in \mathcal{S}} \sqrt{P(k)}\mathbf{w}(k)s(k).
\end{equation}
${\blacktriangleright}$ $s(k),\mathbf{w}(k),P(k)$: data symbol, beamforming weight vector, and transmit power scaling factor for user $k$.

\vspace{-2pt}
\item The signal received by a user $k$ is given by:
\begin{multline}\notag
y(k) = \mathbf{h}(k)\mathbf{x}+z(k)
    = \sqrt{P(k)}\mathbf{h}(k)\mathbf{w}(k)s(k) + \\
    \sum_{j \in \mathcal{S},j \ne k}  \sqrt{P(j)}\mathbf{h}(j)\mathbf{w}(j)s(j)
    +  z(k), \forall k \in \mathcal{U} \text{ or } \mathcal{V}.
\end{multline}
\small
${\blacktriangleright}$ $\mathbf{h}(k)= [h(k,1),...,h(k,I)]\in \mathbb{C}^{1\times I}$ : channel vector for user $k$. \\
${\blacktriangleright}$ $z(k)$: white Gaussian channel noise for user $k$.

\item  To cancel mutual interference, adopt the ZFBF based downlink precoder $ \mathbf{W} = \mathbf{H}^{\dagger} = \mathbf{H}^{*}(\mathbf{HH}^{*})^{-1}$.

\item Power allocation is found by water-filling.
\end{itemize}
\end{minipage}
}

%%% Results %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\headerbox{Associated user group construction}{name=group,column=1,span=2, below=background, bottomaligned = signal}{
\small
\emph{1)} \textbf{Initialization}:
    \begin{itemize}
        \vspace{-2pt}\item[$\blacktriangleright$] Initialize the unselected user set as the sum aggregation of the cellular user set $\mathcal{U}$ and the WiFi user set $\mathcal{V}$, i.e., $\mathcal{T}_i = \mathcal{U} +\mathcal{V}.$
            The number of initial unselected users is denoted by $K$.
        \vspace{-2pt}\item[$\blacktriangleright$] Set the iteration number $i = 1$, and the initial associated user set $\mathcal{S} = \emptyset$.
    \end{itemize}
\vspace{-2pt}
\emph{2)} \textbf{Calculation of channel orthogonal components}:
    \begin{itemize}
        \vspace{-2pt}\item[$\blacktriangleright$] Define the orthogonal components of channel vectors of the unselected users in $\mathcal{T}_{i}$ as $\mathbf{g}_{(i)}$, which are orthogonal to the subspace spanned by the selected users  $\{\mathbf{g}_{(1)},\mathbf{g}_{(2)},...,\mathbf{g}_{(i-1)}\}$.
        \vspace{-2pt}\item[$\blacktriangleright$] Calculate $\mathbf{g}(k)$ for each user $k \in \mathcal{T}_i$ as
        \begin{equation}
            \mathbf{g}(k) =
            \mathbf{h} (k) - \sum_{j=1}^{i-1} \frac {\mathbf{h}_k\mathbf{g}_j^*}{\|\mathbf{g}_{(j)}\|^2} \mathbf{g}_{(j)}
            =  \mathbf{h}(k) \mathbf{I}-\sum_{j=1}^{i-1}
            \frac{\mathbf{g}_{(j)}^{*}\mathbf{g}_{(j)}}{\|\mathbf{g}_{(j)}\|^2},
            \forall k > 1,
        \end{equation}
        where  $g(k) = h(k)$ when $k = 1$.
    \end{itemize}
\vspace{-2pt}
\emph{3)} \textbf{User selection based on semiorthogonality}:
    \small
    \begin{itemize}
        \vspace{-2pt}\item[$\blacktriangleright$] The data rate of user $k$ can be approximated as $\tilde{R}_k(\mathcal{S}) = \log(1+P(k)\|\mathbf{g}(k)\|^2)$, since the selected users are semiorthogonal. When the selected users are strictly orthogonal and $P(k) = P/|\mathcal{S}|$, the original data rate of user $k$, $R_k(\mathcal{S}) = \log (1 + P(k)\|\mathbf{h}(k)\|^2)$, equals to $\tilde{R}_k(\mathcal{S})$.

        \vspace{-2pt}\item[$\blacktriangleright$] Select the optimal user $\pi(i)$ from $\mathcal{T}_i$ as
            $\pi(i) = \arg\max_{k\in{\mathcal{T}_i}} \lambda(k) \|\mathbf{g}(k)\|^2$.
        \vspace{-2pt}\item[] $\lambda(k)$ is the weight factor for user k, which provides flexibility to control the access probability of different user types.
    \end{itemize}
}

